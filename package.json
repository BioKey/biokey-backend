{
  "name": "biokey-backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "startProd": "NODE_ENV=production node ./bin/www",
    "watch": "node_modules/.bin/nodemon ./bin/www",
    "test": "NODE_ENV=test node_modules/.bin/_mocha ./test --recursive --exit",
    "cover": "NODE_ENV=test node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha ./test --recursive",
    "testWin": "SET NODE_ENV=test& node_modules\\.bin\\_mocha .\\test --recursive",
    "coverWin": "SET NODE_ENV=test& node_modules\\istanbul\\lib\\cli.js cover node_modules\\mocha\\bin\\_mocha .\\test --recursive",
    "docs": "node_modules/.bin/apidoc -i routes -o docs",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "gen-data": "node generate-dummy-data.js"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.15.0",
    "confirm-cli": "^0.4.0",
    "cors": "^2.8.4",
    "dotenv": "^5.0.0",
    "express": "^4.13.4",
    "forever": "^0.15.3",
    "helmet": "^3.12.0",
    "jwt-simple": "^0.5.0",
    "mongoose": "^4.4.12",
    "mongoose-long": "^0.1.1",
    "mongoose-paginate": "^5.0.3",
    "morgan": "^1.7.0",
    "nodemon": "^1.9.1",
    "npm": "^5.7.1",
    "passport": "^0.3.2",
    "passport-jwt": "^2.0.0",
    "passport-local": "^1.0.0",
    "q": "^1.5.1",
    "twilio": "^3.11.3"
  },
  "devDependencies": {
    "apidoc": "^0.17.6",
    "aws-sdk": "^2.197.0",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "coveralls": "^3.0.0",
    "istanbul": "^0.4.5",
    "istanbul-coveralls": "^1.0.3",
    "mocha": "^4.0.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.12.1",
    "sinon": "^4.1.2"
  }
}
